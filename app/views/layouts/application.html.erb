<!DOCTYPE html>
<html>
  <head>
    <title>PowerhouseGym</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="container">
    <nav class="navbar navbar-default">
	  <div class="container-fluid">
	    <div class="navbar-header">
	      <a class="navbar-brand" href="<%= root_path %>" data-turbolinks="false" >Powerhouse Gym</a>
	    </div>
	    <ul class="nav navbar-nav">
	      <li><a href="<%= root_path %>" data-turbolinks="false" >Serve</a></li>
	      <li><a href="<%= members_path %>" data-turbolinks="false" >Manage Member</a></li>
	      <li><a href="<%= serves_path %>" data-turbolinks="false" >Serve Report</a></li>
	    </ul>
	  </div>
	</nav>
    <%= yield %>
  </body>
</html>
